<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/home_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Inicio"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/anadir_diario_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Añadir diario"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/resultados_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Resultados"
        tools:layout_editor_absoluteX="217dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/ayuda_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ayuda"
        tools:layout_editor_absoluteX="328dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/como_te_sientes_text"
        android:layout_width="361dp"
        android:layout_height="27dp"
        android:text="¿Como te sientes?"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="84dp" />

    <CalendarView
        android:id="@+id/calendarView"
        android:layout_width="320dp"
        android:layout_height="137dp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="127dp" />
</android.support.constraint.ConstraintLayout>